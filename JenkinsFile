try {
	node {
		stage ('Setup') {
			helper = load "${env.WORKSPACE.split('@')[0]}@script/src/helpers.groovy"
			helper.info("Yay")
		}
	node("selenium"){
		stage ('Checkout'){		
			helper.checkoutRepo("ranorex")
			echo "===accessing Team's repo  ==============="
			
		}
	}
		
	}		
	
} catch(Exception ex) {
	println(ex)
	
    currentBuild.result = 'FAILURE'
} finally {
	echo "===Done  ==============="
}